      "EnvironmentVariables": {
        "MODELS_DIR": "%CURRENT_MODULE_PATH%\\sentiment_model",
        "TF_CPP_MIN_LOG_LEVEL": "3" // 0=Debug, 1=Info, 2=Warning, 3=Error
      },

      "GpuOptions" : {
